Start{
	
	
	Heltall poengsum;
	Sannhet ferdig := usant;
	Setning set;
	
	Gjenta{
		
		Skriv ut: "Please enter your test score percentage, between 0 and 100:";
		Skriv inn: poengsum;
		
		Dersom (poengsum > 90) så {
		
			Skriv ut: "Congratulations, you got an A!";
	
		} ellers {
	
			Dersom (poengsum > 80) så {
		
				Skriv ut: "Good job, you got a B.";
	
			} ellers {
			
				Dersom (poengsum > 70) så {
		
					Skriv ut: "Average, you got a C.";
		
				} ellers {
		
					Dersom (poengsum > 60) så {
		
						Skriv ut: "At least you didn't fail, you got a D.";
		
					} ellers {
	
						Skriv ut: "You failed, you got an F!";				
		
					}
				}
			}
		}
	
		
	Skriv ut: "Do you wish to write in another test score? Type yes to continue";
	Skriv inn: set;	
	Dersom (set != "yes") så {
		Angi ferdig := usant;
	} ellers {
		Angi ferdig := sant;
	}
	
		
	}Medan(ferdig)	
		
}Slutt

